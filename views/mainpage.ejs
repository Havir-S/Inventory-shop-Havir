<%- include('partials/navmain') -%>

    <div class="mainphotos-container slide1">
      <div class="photo anim">
        <img src="/images/gaming1.jpg" alt="photo1">
        <div class="text">By gamers for gamers</div>
      </div>
      <div class="photo anim">
        <img src="/images/gaming2.jpg" alt="photo1">
