                    <p class='orange'><%= keyboards[0].model %></p>
                    <p class='description'><%= keyboards[0].description %></p>
                    <h3 class='orange keyboardprice'><%=keyboards[0].price%><span class='eur'>EUR</span></h3>
                    <button class='other2' type="button" name="button">CHECK OUT</button>
                  </div>
                  </a>
                </div>
                <div class="keyboard-tab">
                  <a href="<%= keyboards[1].url %>">
